{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "double"
    ],
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
